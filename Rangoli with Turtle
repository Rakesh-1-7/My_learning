import turtle
t = turtle.Turtle()
turtle.Screen().bgcolor("black") 

def square(x,y="black"):
  t.shape("turtle")
  t.color(y)
  t.speed(0)
  for i in range(4):
    t.fd(x)
    t.lt(90) 
  t.lt(11)

# #add8e6 - Light Blue
# #ff4500 - Orange Red

for i in range(30):
  square(150,"yellow") 

  # square(100,"red")

t.pencolor("#006994 ")
for i in range(300):
  t.fd(i)
  t.rt(91)
  
